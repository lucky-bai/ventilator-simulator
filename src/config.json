{
    "outputFormat": {
        "primary": [
            ["Primary", ["PC", "PEEP0", "RR", "IT", "I:E"]]
        ],
        "patient": [
            ["Secondary", ["Cr", "HCO3", "FGFO", "FGFA", "PEEP"]],
            ["Patient", ["FGFtot", "VT", "MIP", "FIO2", "PEEPInt", "APEEP", "VCO2", "VO2", "ADS", "AVT", "PaO2", "PaCO2", "CaCO2", "HPlus", "pH"] ]   
        ]
    },

    "variables": {
        "primary": [
            {
                "key": "PC",
                "desc": "Pressure Control Level",
                "unit": "cmH20",
                "range": [
                    0,
                    70
                ],
                "interval": 5,
                "defaultValue": 40
            },
            {
                "key": "PEEP0",
                "desc": "Positive End-Expiratory Pressure Level",
                "range": [
                    0,
                    10
                ],
                "defaultValue": 5
            },
            {
                "key": "RR",
                "desc": "Respiratory Rate",
                "unit": "breaths/min",
                "range": [
                    2,
                    40
                ],
                "defaultValue": 18
            },
            {
                "key": "IT",
                "desc": "Inspiratory Time",
                "unit": "sec",
                "range": [
                    0.5,
                    10
                ],
                "interval": 0.1,
                "defaultValue": 1
            },
            {
                "key": "I:E",
                "desc": "Inspiratory Time:Expiratory Time Ratio"
            }
        ],
        "patient": [
            {
                "key": "H",
                "desc": "Height",
                "unit": "cm",
                "range": [
                    100,
                    250
                ],
                "defaultValue": 140
            },
            {
                "key": "W",
                "desc": "Weight",
                "unit": "kg",
                "range": [
                    40,
                    120
                ],
                "defaultValue": 55
            },
            {
                "key": "Cr",
                "desc": "Lung Compliance",
                "interval": 5,
                "unit": "ml/cmH20",
                "range": [
                    10,
                    70
                ],
                "defaultValue": 15
            },
            {
                "key": "PF",
                "desc": "Ratio of arterial partial pressure of oxygen to fraction of inspired oxygen",
                "range": [
                    100,
                    500
                ],
                "defaultValue": 100
            },
            {
                "key": "HCO3",
                "desc": "Arterial Bicarbonate Concentration",
                "unit": "nM/L",
                "range": [
                    22,
                    28
                ],
                "defaultValue": 24
            },
            {
                "key": "FGFO",
                "desc": "Fresh Gas Oxygen Flow",
                "unit": "L/min",
                "range": [
                    0,
                    15
                ],
                "defaultValue": 8
            },
            {
                "key": "FGFA",
                "desc": "Fresh Gas Air Flow",
                "unit": "L/min",
                "range": [
                    0,
                    15
                ],
                "defaultValue": 2
            },
            {
                "key": "PEEP",
                "desc": "PEEP Valve Setting",
                "unit": "cmH20",
                "range": [
                    0,
                    25
                ],
                "defaultValue": 5
            },
            {
                "key": "pH",
                "desc": "pH",
                "range": [
                    7.3,
                    7.4
                ]
            },
            {
                "key": "PaCO2",
                "desc": "Partial pressure of carbon dioxide",
                "range": [
                    35,
                    45
                ],
                "unit": "mmHg"
            },
            {
                "key": "PaO2",
                "desc": "Partial pressure of oxygen",
                "range": [
                    100,
                    null
                ],
                "unit": "mmHg"
            },
            {
                "key": "PIP",
                "desc": "Peak Inspiratory pressure",
                "range": [
                    0,
                    40
                ],
                "unit": "cmH20"
            },
            {
                "key": "APEEP",
                "desc": "Actual PEEP",
                "unit": "cmH20"
            },
            {
                "key": "VT",
                "desc": "Tidal volume",
                "unit": "ml",
                "range": [
                    50,
                    700
                ]
            },
            {
                "key": "FiO2",
                "desc": "Fraction Inspired Oxygen"
            }
        ]
    }
}